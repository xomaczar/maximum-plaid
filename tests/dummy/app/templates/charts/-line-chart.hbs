{{#plot-dimension height=200 as |dim|}}
{{#with (area dim.width dim.height margin="0 50 72 50") as |plotArea|}}
  {{#plaid-plot
    (time-scale (extent (map-by "transactionDate" responseTimeMean)) (array 0 plotArea.width))
    (linear-scale (extent (map-by "profit" responseTimeMean) toZero=true) (array plotArea.height 0))
    plotArea as |plot|}}



    {{#with (pair-by "transactionDate" "profit" responseTimeMean) as |values|}}
      {{plot.line values stroke="#673AB7" strokeWidth="2" curve=(curve "basis")}}
      {{!-- {{plot.area values fill="#D1C4E9" curve=(curve "basis")}} --}}

      {{!-- {{#plot.bottom-axis ticks=(compute (get (time-interval 'year') 'every') 1)}}
        {{plaid-text 'Timestamps' class='axis-title' x=(div plotArea.width 2) y=40}}
      {{/plot.bottom-axis}} --}}

      {{!-- {{#plot.bottom-axis ticks=(compute (get (time-interval 'minute') 'every') 3)}}
        {{plaid-text 'Timestamps' class='axis-title' x=(div plotArea.width 2) y=40}}
      {{/plot.bottom-axis}} --}}

      {{#plot.bottom-axis ticks=(time-interval 'day') tickFormat="%a"}}
        {{plaid-text 'Timestamps' class='axis-title' x=(div plotArea.width 2) y=40}}
      {{/plot.bottom-axis}}

      {{!-- {{#plot.bottom-axis tickValues=(scale-ticks plot.xScale (time-interval 'month'))}}
        {{plaid-text 'Transaction' class='axis-title' x=(div plotArea.width 2) y=40}}
      {{/plot.bottom-axis}} --}}


      {{#plot.right-axis tickFormat=(format-fn "0.1s") ticks=2}}
        {{!-- {{plaid-text 'Amount' class='axis-title' x=50 y=(div plotArea.height 2) textRotate=90}} --}}
      {{/plot.right-axis}}

    {{/with}}
    {{#with (pair-by "transactionDate" "revenue" responseTimeMean) as |gridValues|}}
      {{plot.line gridValues stroke="#FF7F50" strokeWidth="2" curve=(curve "basis")}}
      {{!-- {{plot.area gridValues fill="#FF7F50" curve=(curve "basis")}} --}}
    {{/with}}
  {{/plaid-plot}}
{{/with}}
{{/plot-dimension}}


{{!-- {{#plaid-plot
  (time-scale (extent (map-by "timestamp" responseTimeMean)) (array 0 plotArea.width))
  (linear-scale (extent (map-by "value" responseTimeMean) toZero=true) (array plotArea.height 0))
  plotArea as |plot|}}

  {{#with (pair-by "timestamp" "value" responseTimeMean) as |values|}}
    {{plot.line values stroke="#673AB7" strokeWidth="2" curve=(curve "basis")}}
    {{plot.area values fill="#D1C4E9" curve=(curve "basis")}}
    {{#plot.bottom-axis values ticks=10}}
      {{plaid-text 'Timestamps' class='axis-title' x=(div plotArea.width 2) y=40}}
    {{/plot.bottom-axis}}
    {{#plot.right-axis values tickFormat=(format-fn "0.1s" suffix="ms") ticks=2}}
      {{plaid-text 'Values' class='axis-title' x=50 y=(div plotArea.height 2) textRotate=90}}
    {{/plot.right-axis}}
  {{/with}}
{{/plaid-plot}} --}}
